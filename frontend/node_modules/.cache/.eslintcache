lly');\r\n        } catch (err) {\r\n            dispatch({\r\n                type: 'FETCH_FAIL',\r\n            });\r\n            toast.error(err);\r\n        }\r\n    };\r\n\r\n    return (\r\n        <div className=\"container small-container\">\r\n   \r\n                <title>User Profile</title>\r\n           \r\n            <h1 className=\"my-3\">User Profile</h1>\r\n            <form onSubmit={submitHandler}>\r\n                <Form.Group className=\"mb-3\" controlId=\"name\">\r\n                    <Form.Label>Name</Form.Label>\r\n                    <Form.Control\r\n                        value={name}\r\n                        onChange={(e) => setName(e.target.value)}\r\n                        required\r\n                    />\r\n                </Form.Group>\r\n                <Form.Group className=\"mb-3\" controlId=\"name\">\r\n                    <Form.Label>Email</Form.Label>\r\n                    <Form.Control\r\n                        type=\"email\"\r\n                        value={email}\r\n                        onChange={(e) => setEmail(e.target.value)}\r\n                        required\r\n                    />\r\n                </Form.Group>\r\n                <Form.Group className=\"mb-3\" controlId=\"password\">\r\n                    <Form.Label>Password</Form.Label>\r\n                    <Form.Control\r\n                        type=\"password\"\r\n                        onChange={(e) => setPassword(e.target.value)}\r\n                    />\r\n                </Form.Group>\r\n                <Form.Group className=\"mb-3\" controlId=\"password\">\r\n                    <Form.Label>Confirm Password</Form.Label>\r\n                    <Form.Control\r\n                        type=\"password\"\r\n                        onChange={(e) => setConfirmPassword(e.target.value)}\r\n                    />\r\n                </Form.Group>\r\n                <div className=\"mb-3\">\r\n                    <Button type=\"submit\">Update</Button>\r\n                </div>\r\n            </form>\r\n        </div>\r\n    );\r\n}\r\n\r\n","D:\\Huntys Git\\NurseryReactApp\\frontend\\src\\screens\\SignupScreen.js",[],[],{"ruleId":"115","severity":1,"message":"116","line":2,"column":26,"nodeType":"117","messageId":"118","endLine":2,"endColumn":32},{"ruleId":"115","severity":1,"message":"119","line":5,"column":8,"nodeType":"117","messageId":"118","endLine":5,"endColumn":14},{"ruleId":"115","severity":1,"message":"120","line":27,"column":13,"nodeType":"117","messageId":"118","endLine":27,"endColumn":21},{"ruleId":"121","severity":1,"message":"122","line":46,"column":8,"nodeType":"123","endLine":46,"endColumn":14,"suggestions":"124"},{"ruleId":"121","severity":1,"message":"125","line":48,"column":8,"nodeType":"123","endLine":48,"endColumn":10,"suggestions":"126"},{"ruleId":"115","severity":1,"message":"127","line":15,"column":12,"nodeType":"117","messageId":"118","endLine":15,"endColumn":17},{"ruleId":"115","severity":1,"message":"128","line":15,"column":19,"nodeType":"117","messageId":"118","endLine":15,"endColumn":27},{"ruleId":"115","severity":1,"message":"129","line":16,"column":9,"nodeType":"117","messageId":"118","endLine":16,"endColumn":25},{"ruleId":"115","severity":1,"message":"130","line":28,"column":12,"nodeType":"117","messageId":"118","endLine":28,"endColumn":27},{"ruleId":"115","severity":1,"message":"131","line":30,"column":14,"nodeType":"117","messageId":"118","endLine":30,"endColumn":27},"no-unused-vars","'Router' is defined but never used.","Identifier","unusedVar","'logger' is defined but never used.","'category' is assigned a value but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'categorySlug'. Either include it or remove the dependency array.","ArrayExpression",["132"],"React Hook useEffect has a missing dependency: 'category'. Either include it or remove the dependency array.",["133"],"'count' is assigned a value but never used.","'setCount'